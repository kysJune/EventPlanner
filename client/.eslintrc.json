{
	"env": {
		"browser": true,
		"commonjs": true,
		"es2021": true
	},
	"extends": ["google", "plugin:react/recommended"],
	"parserOptions": {
		"ecmaVersion": "latest"
	},
	"plugins": ["react"],
	"rules": {
		"semi": [1, "always"],
		"quotes": ["error", "double"],
		"no-console": ["warn", { "allow": ["info", "error"] }],
		"indent": ["error", "tab"],
		"comma-dangle": "off"
	}
}
